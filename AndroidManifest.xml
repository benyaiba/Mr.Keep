<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.password"
    android:versionCode="2"
    android:versionName="0.1.2" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
		<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    	<uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    	<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <activity
            android:name="com.example.password.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.password.AddRecordActivity"
           		  android:label="@string/add_a_record" ></activity>
        <activity android:name="com.example.password.RecordEditActivity"
           		  android:label="@string/edit_a_record" ></activity>
        <activity android:name="com.example.password.MainActivity"
           		  android:label="@string/app_name" ></activity>
        <activity android:name="com.example.password.RecordDetailActivity"
           		  android:label="@string/detail_a_record" ></activity>
        <activity android:name="com.example.password.DataManagementActivity"
           		  android:label="@string/data_management" ></activity>
           		  <activity android:name="com.example.password.LoginPEditActivity"
           		  android:label="@string/login_p_change" ></activity>
    </application>
    

</manifest>
